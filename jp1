node {
    agent any
    def PROJECT_NAME = "Red Alert" // a plain string
    def OWNER_NAME = "Skiff1"
//    def mvnHome
    stage('1Preparation') { // for display purposes
        echo "Preparation"
        }
    stage('2Build') {
        // Run the maven build
        echo "Build"
        echo "Hello ${OWNER_NAME}"
        echo "Hello ${PROJECT_NAME}"
    }
    stage('3Results') {
        echo "Results"
        sh "python --version"
    }
}
